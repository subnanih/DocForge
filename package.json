{
  "name": "docforge",
  "version": "1.0.0",
  "description": "DocForge - Where Great Documentation is Forged. A next-generation, multi-tenant documentation platform with AI-powered features and MCP integration.",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run api\" \"npm run frontend\"",
    "api": "node api/server.js",
    "frontend": "node frontend/server.js",
    "dev": "concurrently \"nodemon api/server.js\" \"nodemon frontend/server.js\"",
    "setup": "node setup.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "axios": "^1.5.0",
    "cookie-parser": "^1.4.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=18.0"
  },
  "keywords": [
    "documentation",
    "saas",
    "multi-tenant",
    "api",
    "mcp",
    "ai-integration",
    "markdown",
    "express",
    "mongodb"
  ],
